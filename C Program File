    if (num1 >= num2 && num1 >= num3)
 {
        printf("%d is the largest number.\n", num1);
    }
 else if (num2 >= num1 && num2 >= num3)
 {
        printf("%d is the largest number.\n", num2);
    }
 else
 {
        printf("%d is the largest number.\n", num3);
    }
}
 
