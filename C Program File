    p=2;
    b=1;
    printf("\n Enter a N number= ");
    scanf("%d",&n);
   printf("\n Enter a X value = ");
   scanf("%d",&x);
   for ( i = 1; i <=n ; i++)
   {
        p=p+3;
        b=b+2;
        a=pow(x,p);
        f=1;
            for ( j = 1; j <=n; j++)
            {
                f=f*j;
            }
            if (i%2==0)
            {
                s=s-(1.0*a)/b;
            }
            else
            {
                s=s+(1.0*a)/b;
            }
    }
