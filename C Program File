#include <stdio.h>
#include <conio.h>
#include<math.h>
void main()
{
    int i,x,n,a,p,b;
    float s;
    p=1;
    b=5;
    printf("\n Enter a N number= ");
    scanf("%d",&n);
   printf("\n Enter a X value = ");
   scanf("%d",&x);
   for ( i = 1; i <=n; i++)
   {
    p=p+3;
    b=b+4;
    a=pow(x,p);
    if (i%2==0)
            {
                s=s-(1.0*a)/b;
            }
            else
            {
                s=s+(1.0*a)/b;
            }
    
   }
   printf("\n sum= %.2f",s);
}

