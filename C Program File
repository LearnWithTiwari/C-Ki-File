#include<conio.h>
int main()
{
int n, i, factorial;
factorial=1;
printf("\n\t Enter a number: ");
scanf("%d", &n);
if (n < 0) 
{
printf("\n Factorial of a negative number doesn't exist.");
} 
else 
{
for (i = 1; i <= n; i++) 
{
factorial = factorial*i;  
}
printf("Factorial of %d is: %d", n, factorial);
}
